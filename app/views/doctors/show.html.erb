<section id="doctor-info">
  <h1><%= @doctor.name %></h1>
  <p>Specialty: <%= @doctor.specialty %></p>
  <p>University: <%= @doctor.university %></p>
  <p>Hospital: <%= @doctor.hospital_name %></p>
</section>
<h3>Patients</h3>
<ul id="patients-list">
  <% @doctor.patients.each do |patient| %>
    <li><%= patient.name %></li>
  <% end %>
</ul>